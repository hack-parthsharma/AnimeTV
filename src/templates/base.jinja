<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

	<!-- CSS -->
	<link rel="stylesheet" href="{{ url_for('static', filename='css/bootstrap-reboot.min.css') }}">
	<link rel="stylesheet" href="{{ url_for('static', filename='css/bootstrap-grid.min.css') }}">
	<link rel="stylesheet" href="{{ url_for('static', filename='css/owl.carousel.min.css') }} ">
	<link rel="stylesheet" href="{{ url_for('static', filename='css/slider-radio.css') }} ">
	<link rel="stylesheet" href="{{ url_for('static', filename='css/select2.min.css') }} ">
	<link rel="stylesheet" href="{{ url_for('static', filename='css/magnific-popup.css') }} ">
	<link rel="stylesheet" href="{{ url_for('static', filename='css/plyr.css') }} ">
	<link rel="stylesheet" href="{{ url_for('static', filename='css/main.css') }} ">

    {% block cssArea %}
    {% endblock cssArea %}


	<!-- Favicons -->
	<link rel="icon" type="image/png" href="icon/favicon-32x32.png" sizes="32x32">
	<link rel="apple-touch-icon" href="icon/favicon-32x32.png">

	<meta name="description" content="">
	<meta name="keywords" content="">
	<meta name="author" content="Dmitry Volkov">
	<title> {% block contentTitle %} AnimeTv {% endblock contentTitle %} </title>

</head>

<body>
	
    {% block contentBody %}
        {% include 'snippets/header.jinja' %}

    {% endblock contentBody %}

    <script src="{{ url_for('static', filename='js/jquery-3.5.1.min.js') }}"></script>
	<script src="{{ url_for('static', filename='js/bootstrap.bundle.min.js') }}"></script>
	<script src="{{ url_for('static', filename='js/owl.carousel.min.js') }}"></script>
	<script src="{{ url_for('static', filename='js/slider-radio.js') }}"></script>
	<script src="{{ url_for('static', filename='js/select2.min.js') }}"></script>
	<script src="{{ url_for('static', filename='js/smooth-scrollbar.js') }}"></script>
	<script src="{{ url_for('static', filename='js/jquery.magnific-popup.min.js') }}"></script>
	<script src="{{ url_for('static', filename='js/plyr.min.js') }}"></script>
	<script src="{{ url_for('static', filename='js/main.js') }}"></script>

    {% block javascriptArea %}
    {% endblock javascriptArea %}
</body>
</html>